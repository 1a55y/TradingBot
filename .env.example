# TopStepX API Credentials
# Get these from your TopStep dashboard
TOPSTEP_API_KEY=your_api_key_here
TOPSTEP_API_SECRET=your_api_secret_here
TOPSTEP_USERNAME=your_username
TOPSTEP_USER_ID=your_user_id
TOPSTEP_ACCOUNT_ID=your_account_id
TOPSTEP_MARKET=CME_TOB

# Trading Settings
# Set to True for paper trading, False for live trading
PAPER_TRADING=True

# Account IDs (optional - defaults are provided)
# TOPSTEP_STEP1_ACCOUNT_ID=9203477
# TOPSTEP_PRACTICE_ACCOUNT_ID=9156522

# Risk Management (optional - defaults are provided)
# DEFAULT_ACCOUNT_SIZE=150000

# Mock Trading Parameters (optional - only used in mock mode)
# MOCK_VOLATILITY=0.001
# MOCK_TREND=0.00001
# MOCK_BASE_WIN_PROBABILITY=0.45

# Contract Selection
# Available options: MNQ, NQ, MES, ES, MGC, GC
# MNQ = Micro E-mini Nasdaq (recommended for beginners)
# NQ = E-mini Nasdaq
# MES = Micro E-mini S&P 500
# ES = E-mini S&P 500
# MGC = Micro Gold
# GC = Gold Futures
TRADING_CONTRACT=MNQ

# API URLs (leave as default unless instructed otherwise)
TOPSTEP_API_BASE_URL=https://api.topstepx.com
TOPSTEP_RTC_BASE_URL=https://rtc.topstepx.com
TOPSTEP_WS_DATA_URL=wss://rtc.topstepx.com/hubs/market
TOPSTEP_WS_TRADING_URL=wss://rtc.topstepx.com/hubs/user